<p id="notice"><%= notice %></p>

<h1>Fila Mentors</h1>

<table>
  <thead>
    <tr>
      <th>Mentor</th>
      <th>Habilidade</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fila_mentors.each do |fila_mentor| %>
      <tr>
        <td><%= fila_mentor.mentor_id %></td>
        <td><%= fila_mentor.habilidade %></td>
        <td><%= link_to 'Show', fila_mentor %></td>
        <td><%= link_to 'Edit', edit_fila_mentor_path(fila_mentor) %></td>
        <td><%= link_to 'Destroy', fila_mentor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fila Mentor', new_fila_mentor_path %>
